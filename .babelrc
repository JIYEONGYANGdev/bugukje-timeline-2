{
    "presets": [
      [
        "@babel/preset-react",
        { "runtime": "automatic" }
      ],
      [
        "@babel/preset-env",
        { "modules": false, "useBuiltIns": "usage" }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "fileName": true,
          "displayName": true,
          "pure": true
        }
      ]
    ]
  }